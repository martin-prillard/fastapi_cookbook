{
  "id": null,
  "title": "FastAPI Monitoring",
  "tags": ["fastapi", "prometheus"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "panels": [
    {
      "type": "graph",
      "title": "Requests per second",
      "targets": [
        {"expr": "rate(api_requests_total[1m])", "legendFormat": "req/s"}
      ],
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8}
    },
    {
      "type": "graph",
      "title": "95th percentile latency",
      "targets": [
        {"expr": "histogram_quantile(0.95, rate(api_request_latency_seconds_bucket[1m]))", "legendFormat": "p95"}
      ],
      "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8}
    }
  ]
}
